{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.5.1"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        },
        {
            "source": "/robots.txt",
            "destination": "/api/robots.txt"
        },
        {
            "source": "/favicon.ico",
            "destination": "/api/favicon.ico"
        }
    ]
}
