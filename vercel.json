{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.5.1"
        },
        "api/assets.php": {
            "runtime": "vercel-php@0.5.1"
        }
    },
    "rewrites": [
        {
            "source": "/assets/css/:path*",
            "destination": "/api/assets.php?type=css&file=:path*"
        },
        {
            "source": "/assets/js/:path*",
            "destination": "/api/assets.php?type=js&file=:path*"
        },
        {
            "source": "/robots.txt",
            "destination": "/api/assets.php?file=robots.txt"
        },
        {
            "source": "/favicon.ico",
            "destination": "/api/assets.php?file=favicon.ico"
        },
        {
            "source": "/(.*)",
            "destination": "/api/index.php"
        }
    ]
}
